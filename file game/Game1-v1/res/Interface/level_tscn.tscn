script/source = "extends Control

var LvLbtm = preload(\"res://res/Interface/level_button.tscn\")
# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	for level in lvldata.leveldata.get_sections():
		var levelbutton = LvLbtm.instantiate()
		levelbutton.level = level
		levelbutton.unlock = lvldata.leveldata.get_value(level,\"unlock\")
		levelbutton.time = lvldata.leveldata.get_value(level,\"time\")
		$Panel/VBoxContainer.add_child(levelbutton)



func _on_button_pressed() -> void:
	get_tree().change_scene_to_packed(load(\"res://res/scene/level.tscn\"))
	
"

